{
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/OpenTwinCities/otc-blogs-capitol-code-theme"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "1.*",
    "johnpbloch/wordpress": "4.*",
    "OpenTwinCities/otc-blogs-capitol-code-theme": "1.*",
    "wpackagist-plugin/akismet": "3.*",
    "wpackagist-plugin/googleanalytics": "1.*",
    "wpackagist-plugin/google-sitemap-generator": "4.*",
    "wpackagist-plugin/mailchimp-for-wp": "3.*",
    "wpackagist-plugin/ninja-forms": "2.*",
    "wpackagist-plugin/table-of-contents-plus": "1509",
    "wpackagist-plugin/wonderm00ns-simple-facebook-open-graph-tags": "1.*",
    "wpackagist-plugin/wp-github-sync": "1.*",
    "wpackagist-plugin/wp-open-graph": "1.*",
    "wpackagist-plugin/wp-markdown": "1.*",
    "wpackagist-theme/twentyfifteen": "1.*",
    "wpackagist-theme/ward": "1.*"
   },
   "extra": {
    "wordpress-install-dir": "public/wp",
    "installer-paths": {
      "public/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "public/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "public/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    }
   }
}
